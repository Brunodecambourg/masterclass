                        </div><!-- .masterclass-count -->
                    </div><!-- .masterclass-wrap -->
                </div><!-- .masterclass-stats -->
            </div><!-- .col -->

            <div class="col-12 col-lg-6 d-flex align-content-center mt-5 mt-lg-0">
