<!-- Header -->
<header id="header" class="u-header u-header--abs-top-md u-header--show-hide-md u-header--untransitioned" data-header-fix-moment="500" data-header-fix-effect="slide">
  <div class="u-header__section custom_header">
    <div id="logoAndNav" class="container">
      <!-- Nav -->
      <nav class="js-mega-menu navbar navbar-expand-md u-header__navbar hs-menu-initialized hs-menu-horizontal">
        <!-- Logo -->
        <a class="navbar-brand u-header__navbar-brand u-header__navbar-brand-top-space custom_logo-2" href="/landing/bishal" aria-label="Front">
          <%= image_tag asset_pack_path('init/bootstrap/theme/svg/logos/logo-projects-locaux-blc.svg'), :alt => "Logo", :class => " custom_logo" %>
        </a>
        <!-- End Logo -->
      </nav>
      <!-- End Nav -->
    </div>
  </div>
</header>
<!-- End Header -->

<!-- Main -->
<main id="content" role="main">
  <!-- Front for -->
  <div class="container padding-top u-space-3-bottom">
    <div class="w-md-80 w-lg-50 mx-auto mb-9">
      <!-- Title -->
      <div class="mb-3">
        <h3 class="text-primary text-center font-weight-bold">Vos premiers pas sur Projets-Locaux.fr</h3>
      </div>

      <!-- Link -->
      <a class="media align-items-center shadow-sm p-4" href="/landing/bishal3">
        <div class="d-flex pr-4">
          <span class="u-icon u-icon-primary--air u-icon--lg rounded-circle">
            <span class="fa fa-plus u-icon__inner"></span>
          </span>
        </div>

        <span class="media-body">
          <h3 class="h5">Créer un espace de travail </h3>
          <p class="mb-1" style="font-size:0.8rem">Commencer dès à présent à enregistrer vos porteurs de projets locaux</p>  
        </span>

        <span class="fa fa-angle-right m-l-10"></span> 
      </a>
      <!-- End Link -->

      <!-- Divider -->
      <div class="my-2">
      </div>
      <!-- End Divider -->

      <!-- Link -->
      <a class="media align-items-center shadow-sm p-4" href="/landing/bishal4">
        <div class="d-flex pr-4">
          <span class="u-icon u-icon-primary--air u-icon--lg rounded-circle">
            <span class="fa fa-search u-icon__inner"></span>
          </span>
        </div>

        <span class="media-body">
          <h3 class="h5">Trouver un espace de travail </h3>
          <p class="mb-1" style="font-size:0.8rem">Affichez les listes de porteurs de projets des autres utilisateurs autour de vous (BGE, CCI, Collectivités territoriales, etc.)</p>  
        </span>

        <span class="fa fa-angle-right m-l-10"></span> 
      </a>
      <!-- End Link -->


    </div>
    <!-- End Title -->
  
  </div>
  <!-- End Front for -->
</main>
<!-- End Main -->